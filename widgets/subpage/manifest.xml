<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<widget>
	<name>subpage</name>
	<label>Sub-page</label>
	<description>Sub-page</description>
	<version>1.0</version>
	<category>Advanced widgets</category>
	<settings>
    <setting label="" type="separator" />
    <setting id="view_mode" label="View Mode" type="list" default="">
      <option key="" value="Standard" />
      <option key="dialog" value="Dialog" />
      <option key="dialogmodal" value="Dialog Modal" />
      <option key="hideshow" value="Hide / Show" />
    </setting>
    <setting id="hidetitledialog" label="Hide title dialog" type="list" default="false">
      <option key="true" value="Yes" />
      <option key="false" value="No" />
    </setting>
    <setting id="picture-dialog" label="Picture show dialog" type="picture" />
    <setting id="put_left" label="Put on the Left side" type="list" default="false">
      <option key="true" value="Yes" />
      <option key="false" value="No" />
    </setting>
    <setting id="put_top" label="Put on top" type="list" default="false">
      <option key="true" value="Yes" />
      <option key="false" value="No" />
    </setting>
    <setting label="" type="separator" />
		<setting id="subpage" label="Sub-page" type="subpage" />
	</settings>
	<feedbacks>
	</feedbacks>
</widget>