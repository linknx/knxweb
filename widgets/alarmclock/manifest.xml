<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<widget>
	<name>alarmclock</name>
  <label>Alarm Clock</label>
  <description>Alarm Clock</description>
  <version>1.1</version>
  <category>Various widgets</category>
  <settings>
    <setting id="user" label="Alarmclock User" type="text" />
    <setting id="bgcolor" label="Background color" type="color" default="#000000" />
    <setting id="textcolor" label="Text color" type="color" default="#FFFFFF" />
    <setting id="icone" label="Icone Type" type="list" default="White" >
			<option key="White" value="White" />
      <option key="Black" value="Black" />
		</setting>
    <setting label="Action Alarm Clock" type="comment" />
		<setting id="idrule" label="Id Alarm Rule" type="text" />
		<setting id="active-action" label="Action" type="action" />
  </settings>
  <feedbacks></feedbacks>
</widget>